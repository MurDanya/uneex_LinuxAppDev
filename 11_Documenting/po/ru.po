#: ../src/guess.c:89
#, c-format
msgid "Guessing a number from 1 to 100.\n"
msgstr "Угадывание числа от 1 до 100.\n"

#: ../src/guess.c:90
#, c-format
msgid "Usage:\n"
msgstr "Используйте:\n"

#: ../src/guess.c:91
#, c-format
msgid "    --help  Show help message\n"
msgstr "    --help  Показать сообзение справки\n"

#: ../src/guess.c:92
#, c-format
msgid "    -r      Enable Roman numbers\n"
msgstr "    -r      Включить Римские числа\n"

#: ../src/guess.c:100
#, c-format
msgid "Think of a number between 1 and 100\n"
msgstr "Придумайте число от 1 до 100\n"

#: ../src/guess.c:108
#, c-format
msgid "Is the number greater then %s?\n"
msgstr "Ваше число больше чем %s\n"

#: ../src/guess.c:113
msgid "y"
msgstr "д"

#: ../src/guess.c:113
msgid "yes"
msgstr "да"

#: ../src/guess.c:117
msgid "n"
msgstr "н"

#: ../src/guess.c:117
msgid "no"
msgstr "нет"

#: ../src/guess.c:123
#, c-format
msgid "Enter yes(y) or no(n)\n"
msgstr "Введите да(д) или нет(н)\n"

#: ../src/guess.c:127
#, c-format
msgid "You thought of the number %s\n"
msgstr "Вы загадали число %s\n"
